{
  "base_url": "https://api.crocdb.net",
  "endpoints": {
    "search": {
      "method": "POST",
      "path": "search",
      "body_model": "SearchRequest",
      "response_model": "SearchResults"
    },
    "search_get": {
      "method": "GET",
      "path": "search",
      "query_params": [
        {
          "name": "search_key",
          "type": "String",
          "required": false
        },
        {
          "name": "platforms",
          "type": "String",
          "required": false
        },
        {
          "name": "regions",
          "type": "String",
          "required": false
        },
        {
          "name": "max_results",
          "type": "Int",
          "required": false,
          "default": 50
        },
        {
          "name": "page",
          "type": "Int",
          "required": false,
          "default": 1
        }
      ],
      "response_model": "SearchResults"
    },
    "get_entry": {
      "method": "POST",
      "path": "entry",
      "body_model": "EntryRequest",
      "response_model": "EntryResponse"
    },
    "get_random_entry": {
      "method": "POST",
      "path": "entry/random",
      "response_model": "EntryResponse"
    },
    "get_random_entry_get": {
      "method": "GET",
      "path": "entry/random",
      "response_model": "EntryResponse"
    },
    "get_platforms": {
      "method": "GET",
      "path": "platforms",
      "response_model": "PlatformsResponse"
    },
    "get_regions": {
      "method": "GET",
      "path": "regions",
      "response_model": "RegionsResponse"
    },
    "get_info": {
      "method": "GET",
      "path": "info",
      "response_model": "DatabaseInfo"
    }
  },
  "request_models": {
    "SearchRequest": {
      "fields": {
        "search_key": {
          "type": "String",
          "nullable": true
        },
        "platforms": {
          "type": "List<String>",
          "nullable": false
        },
        "regions": {
          "type": "List<String>",
          "nullable": false
        },
        "rom_id": {
          "type": "String",
          "nullable": true
        },
        "max_results": {
          "type": "Int",
          "nullable": false
        },
        "page": {
          "type": "Int",
          "nullable": false
        }
      }
    },
    "EntryRequest": {
      "fields": {
        "slug": {
          "type": "String",
          "nullable": false
        }
      }
    }
  },
  "response_models": {
    "ApiResponse": {
      "wrapper": "ApiResponse",
      "data_path": "data",
      "fields": {
        "info": {
          "type": "ApiInfo",
          "nullable": false
        },
        "data": {
          "type": "T",
          "nullable": false
        }
      }
    },
    "SearchResults": {
      "fields": {
        "results": {
          "type": "List<RomEntry>",
          "nullable": false
        },
        "current_results": {
          "type": "Int",
          "nullable": true
        },
        "total_results": {
          "type": "Int",
          "nullable": true
        },
        "current_page": {
          "type": "Int",
          "nullable": true
        },
        "total_pages": {
          "type": "Int",
          "nullable": true
        }
      }
    },
    "EntryResponse": {
      "fields": {
        "entry": {
          "type": "RomEntry",
          "nullable": true
        }
      }
    },
    "PlatformsResponse": {
      "fields": {
        "platforms": {
          "type": "Map<String,PlatformData>",
          "nullable": false
        }
      }
    },
    "RegionsResponse": {
      "fields": {
        "regions": {
          "type": "Map<String,String>",
          "nullable": false
        }
      }
    },
    "DatabaseInfo": {
      "fields": {
        "total_entries": {
          "type": "Int",
          "nullable": false
        }
      }
    }
  }
}

